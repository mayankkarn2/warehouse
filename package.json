{
  "name": "warehouse-management",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/cds": "^8",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-products": "cds watch --open products/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy warehouse-management --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "cdsc": {
      "severities": {
        "odata-spec-violation-no-key": "Warning"
      }
    },
    "requires": {
      "auth": "xsuaa",
      "destinations": true
    }
  },
  "sapux": [
    "app/products",
    "app/products"
  ]
}
